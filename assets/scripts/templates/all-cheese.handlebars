

<div class="cheese section row">
  <div class="container cheese-handlebars">
    {{#each cheeses}}
      <div class="{{this.family}}-cheese item">
        <div class="{{this.name}}">
          <h2>{{this.name}}</h2>
          <button type="button" data-id="{{this.id}}" data-type="{{this.family}}" class="btn btn-primary get-details" data-toggle="modal" data-target="#get-details-modal">Get Details</button>
        </div>
      </div>
    {{/each}}
  </div>
</div>
